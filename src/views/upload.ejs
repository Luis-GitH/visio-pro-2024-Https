<% include partials/head %>
  </head>

  <body>
    <% include partials/header %>
      <div class='container mx-auto '>
        <hr>

        <div class="card">
          <div class="card-body">
            <h5 class="card-header mb-2">Informativo</h5>

            <p class="card-header">Importados todos los datos de alumnos
            </p>
            <p class="card-text">
              <% if(resultado >0){ %>
                <div class="alert alert-success" role="alert">
                  &nbsp;&nbsp;&nbsp;&nbsp;Se han importado <% resultado %> CMFs de <b>upload </b>
                
                  &nbsp;&nbsp;&nbsp;&nbsp;Revisa la bandeja de <b> <i> correo no deseado</i></b> si ves que no llega
                  <br>
                </div>
                <% } else {%>
                  <div class="alert alert-danger" role="alert">
                    &nbsp;&nbsp;&nbsp;&nbsp;No hay CMFs para importar<br>
                   
                  </div>
                  <% } %>
            </p>
            <p class="mx-auto">

              <a class="btn btn-primary btn-sm" href="/panel">Continuar</a>
            </p>
          </div>
        </div>
      </div>

      <% include partials/footer %>
  </body>

  </html>